<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE edl PUBLIC "-//OPENEMULATOR//DTD EDL 1.0//EN" "http://www.openemulator.org/DTD/edl-1.0.dtd">
<edl version="1.0" label="Terminal" image="images/Generic/Terminal.png" description="A generic terminal that can be connected to any RS-232 port.">

<device id="terminal" label="Terminal" image="images/Generic/Terminal.png">
</device>
<connector id="terminal.connector" type="RS-232 Port"/>
<component id="terminal.io" class="Terminal">
    <property name="device" ref="terminal"/>
    <property name="charset" ref="terminal.charset"/>
    <property name="width" value="24"/>
    <property name="height" value="40"/>
</component>
<component id="terminal.charset" class="CharsetLoader">
    <property name="format" value="Signetics"/>
    <property name="image" src="${resourcePath}/roms/Signetics/Signetics 2513 Video ROM.bin"/>
</component>

</edl>
