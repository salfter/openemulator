<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE edl PUBLIC "-//OPENEMULATOR//DTD EDL 1.0//EN" "http://www.openemulator.org/DTD/edl-1.0.dtd">
<edl version="1.0" generator="OpenEmulator" build="700" label="RD CFFA1" image="images/R&amp;D/R&amp;D CFFA1.png" description="The CFFA1 interface card (www.dreher.net) is used for accessing CompactFlash devices on an Apple-1 computer. It also provides 32kiB of RAM at $1000-$8FFF. To start the file manager, enter 9000R in the monitor.">
    
    <device id="rdCFFA1" label="R&amp;D CFFA1" image="images/R&amp;D/R&amp;D CFFA1.png">
        <setting ref="rdCFFA1.io" name="forceWriteProtected" label="Force Write Protected" type="checkbox"/>
    </device>
    <connector id="rdCFFA1.connector" type="Apple-1 Expansion Port">
        <inlet ref="rdCFFA1.io" property="memoryBus" outletRef="memoryBus"/>
        <inlet ref="rdCFFA1.ram" property="controlBus" outletRef="controlBus"/>
    </connector>
    <component id="rdCFFA1.io" class="RDCFFA1">
        <property name="device" ref="rdCFFA1"/>
        <property name="ram" ref="rdCFFA1.ram"/>
        <property name="rom" ref="rdCFFA1.rom"/>
        <property name="memoryBus" ref=""/>
        <property name="image" value=""/>
        <property name="forceWriteProtected" value="0"/>
    </component>
    <component id="rdCFFA1.ram" class="RAM">
        <property name="controlBus" ref=""/>
        <property name="image" data="${packagePath}/${id}.bin"/>
        <property name="size" value="0x8000"/>
    </component>
    <component id="rdCFFA1.rom" class="ROM">
        <property name="image" data="${resourcePath}/roms/R&amp;D/CFFA1V1.0.bin"/>
    </component>
    
</edl>
