<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE edl PUBLIC "-//OPENEMULATOR//DTD EDL 1.0//EN" "http://www.openemulator.org/DTD/edl-1.0.dtd">
<edl version="1.0" generator="OpenEmulator" framework="736" label="Apple Silentype Interface Card" image="images/Apple/Apple Synch Printer Interface Card.png" description="The Apple Silentype Printer Interface Card connects an Apple Silentype printer to an Apple II computer. It is usually installed in slot 1.">
    
    <device id="appleSilentypeCard" label="Apple Silentype Interface Card" image="images/Apple/Apple Synch Printer Interface Card.png">
    </device>
    <connector id="appleSilentypeCard.connector" type="Apple II Slot">
        <inlet ref="appleSilentypeCard.memory" property="memoryBus" outletRef="memoryBus"/>
    </connector>
    <port id="appleSilentypeCard.port" ref="" type="Apple Silentype Port" group="peripherals" label="Silentype Port" image="images/Connectors/DE9 Female.png"/>
    <component id="appleSilentypeCard.io" class="FloatingBus">
    </component>
    <component id="appleSilentypeCard.memory" class="AppleIISlotMemory">
        <property name="memoryBus" ref=""/>
        <property name="memory" ref="appleSilentypeCard.rom"/>
        <property name="en" value="0"/>
    </component>
    <component id="appleSilentypeCard.rom" class="ROM">
        <property name="memoryImage" data="${resourcePath}/roms/Apple/341-0039-00.bin"/>
    </component>
    
</edl>
