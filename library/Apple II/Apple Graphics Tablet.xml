<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE edl PUBLIC "-//OPENEMULATOR//DTD EDL 1.0//EN" "http://www.openemulator.org/DTD/edl-1.0.dtd">
<edl version="1.0" label="Apple Graphics Tablet" image="images/Apple/Apple Graphics Tablet.png" description="The Apple Graphics Tablet is an digitizing input device for Apple II computers. It must be connected to an Apple Graphics Tablet Interface Card.">
    
    <device id="appleGraphicsTablet" label="Apple Graphics Tablet" image="images/Apple/Apple Graphics Tablet.png">
    </device>
    <connector id="appleGraphicsTablet.connector" type="Apple Graphics Tablet Port">
    </connector>
    <component id="appleGraphicsTablet.graphicsTablet" class="AppleGraphicsTablet">
        <property name="device" ref="appleGraphicsTablet"/>
        <property name="imagePath" value="${resourcePath}/images/Apple/Apple Graphics Tablet View.png"/>
        <property name="audioCodec" ref="appleGraphicsTablet.audioCodec"/>
    </component>
    <component id="appleGraphicsTablet.audioCodec" class="AudioCodec">
        <property name="audio" ref="audio"/>
        <property name="controlBus" ref=""/>
        <property name="timeAccuracy" value="0.000001"/>
        <property name="filterSize" value="17"/>
        <property name="lowFrequency" value="20"/>
        <property name="highFrequency" value="10000"/>
    </component>
    
</edl>
