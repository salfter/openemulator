<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE edl PUBLIC "-//OPENEMULATOR//DTD EDL 1.0//EN" "http://www.openemulator.org/DTD/edl-1.0.dtd">
<edl version="1.0" generator="OpenEmulator" framework="736" label="Apple Silentype" image="images/Apple/Apple Silentype.png" description="The Apple Silentype was Apple's first printer. It can be connected to an Apple II computer (through the Apple II Silentype Interface Card) or an Apple III computer (directly).">
    
    <device id="appleSilentype" label="Apple Silentype" image="images/Apple/Apple Silentype.png">
    </device>
    <connector id="appleSilentype.connector" type="Apple Silentype Port">
        <inlet ref="appleSilentype.io" property="mmu" outletRef="mmu"/>
    </connector>
    <component id="appleSilentype.canvas" class="AppleSilentype">
        <property name="device" ref="appleSilentype"/>
        <property name="imagePath" value="${resourcePath}/images/print.png"/>
    </component>
    
</edl>
