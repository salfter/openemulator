<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE edl PUBLIC "-//OPENEMULATOR//DTD EDL 1.0//EN" "http://www.openemulator.org/DTD/edl-1.0.dtd">
<edl version="1.0" label="Videx UltraTerm" image="images/Videx/Videx UltraTerm.png" description="The Videx UltraTerm is an extended column video card for Apple II computers. It can be installed in slots 0 to 7.">
    
    <device id="videxUltraTerm" label="Videx UltraTerm" image="images/Videx/Videx UltraTerm.png">
    </device>
    <connector id="videxUltraTerm.connector" type="Apple II Slot">
        <inlet ref="videxUltraTerm.memory" property="memoryBus" outletRef="memoryBus"/>
    </connector>
    <component id="videxUltraTerm.io" class="FloatingBus">
    </component>
    <component id="videxUltraTerm.memory" class="AppleIISlotMemory">
        <property name="memoryBus" ref=""/>
        <property name="memory" ref="videxUltraTerm.rom"/>
        <property name="en" value="0"/>
    </component>
    <component id="videxUltraTerm.rom" class="ROM">
        <property name="memoryImage" data="${resourcePath}/roms/Videx/Videx UltraTerm ROM.bin"/>
    </component>
    
</edl>
