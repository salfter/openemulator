<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE edl PUBLIC "-//OPENEMULATOR//DTD EDL 1.0//EN" "http://www.openemulator.org/DTD/edl-1.0.dtd">
<edl version="1.0" label="Apple Grahpics Tablet Interface Card" image="images/Apple/Apple Graphics Tablet Interface Card.png" description="The Apple Graphics Tablet Interface Card connects an Apple Graphics Tablet to an Apple II computer. It is usually installed in slot 5.">
    
    <device id="appleGraphicsTabletCard" label="Apple Graphics Tablet Interface Card" image="images/Apple/Apple Graphics Tablet Interface Card.png">
    </device>
    <connector id="appleGraphicsTabletCard.connector" type="Apple II Slot">
        <inlet ref="appleGraphicsTabletCard.memory" property="memoryBus" outletRef="memoryBus"/>
    </connector>
    <port id="appleGraphicsTabletCard.port" ref="" type="Apple Graphics Tablet Port" group="peripherals" label="Graphics Tablet Port" image="images/Connectors/Header1x6.png">
    </port>
    <component id="appleGraphicsTabletCard.io" class="FloatingBus">
    </component>
    <component id="appleGraphicsTabletCard.memory" class="AppleIISlotMemory">
        <property name="memoryBus" ref=""/>
        <property name="memory" ref="appleGraphicsTabletCard.rom"/>
        <property name="en" value="0"/>
    </component>
    <component id="appleGraphicsTabletCard.rom" class="ROM">
        <property name="memoryImage" data="${resourcePath}/roms/Apple/341-0026-00.bin"/>
    </component>
    
</edl>
