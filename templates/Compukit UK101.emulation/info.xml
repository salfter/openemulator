<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dml PUBLIC "-//OPENEMULATOR//DTD DML 1.0//EN" "http://www.openemulator.org/DTDs/dml-1.0.dtd">
<dml version="1.0" label="Compukit UK101" image="Compukit UK101.png" description="" group="Early microcomputers">
<device name="host">
    <component name="audio" class="HostAudio">
        <property name="runTime" value="0.0" />
        <property name="isPaused" value="0" />
    </component>
    <component name="video" class="HostVideo">
        <property name="window" value="-1 -1 768 576" />
    </component>
    <component name="hid" class="HostHID" />
    <component name="port" class="HostPort" />
</device>

<device name="dev0" label="Apple II">
    <inlet ref="video.monitor" type="AppleIICompositeVideo" category="peripheral" label="Video Connector" image="Apple II Video Connector.png" />
    <inlet ref="game.port" type="AppleIIGame" category="peripheral" label="Game Port" image="Apple II Game Port.png" />
    <inlet ref="slot.slot0" type="AppleIISlot" category="expansion" label="Slot 0" image="Apple II Slot.png" />
    <inlet ref="slot.slot1" type="AppleIISlot" category="expansion" label="Slot 1" image="Apple II Slot.png" />
    <inlet ref="slot.slot2" type="AppleIISlot" category="expansion" label="Slot 2" image="Apple II Slot.png" />
    <inlet ref="slot.slot3" type="AppleIISlot" category="expansion" label="Slot 3" image="Apple II Slot.png" />
    <inlet ref="slot.slot4" type="AppleIISlot" category="expansion" label="Slot 4" image="Apple II Slot.png" />
    <inlet ref="slot.slot5" type="AppleIISlot" category="expansion" label="Slot 5" image="Apple II Slot.png" />
    <inlet ref="slot.slot6" type="AppleIISlot" category="expansion" label="Slot 6" image="Apple II Slot.png" />
    <inlet ref="slot.slot7" type="AppleIISlot" category="expansion" label="Slot 7" image="Apple II Slot.png" />
    <inlet ref="video.romSocket" type="AppleIIVideoROM" category="expansion" label="Video ROM Socket" image="Apple II Video ROM Socket.png" />
    <inlet ref="system.cpuSocket" type="AppleIICPU" category="expansion" label="CPU Socket" image="Apple II CPU Chip Socket.png" />
    <inlet ref="mmu.romSocket" type="AppleIIROM" category="expansion" label="ROM Socket" image="Apple II ROM Socket.png" />
    <component name="system" class="AppleIISystem">
        <property name="cpuSelect" value="0" />
        <property name="cycleClockInc" value="14" />
        <property name="scanlineClockInc" value="2" />
        <connection name="hostAudio" ref="host::audio" />
        <connection name="cpu" ref="cpu" />
        <connection name="cpuExternal" ref="" />
        <connection name="cpuSocket" ref="" />
    </component>
    <component name="cpu" class="MOS6502">
        <connection name="memory" ref="memoryMap" />
        <property name="a" value="0" />
        <property name="x" value="0" />
        <property name="y" value="0" />
        <property name="s" value="0" />
        <property name="p" value="0" />
        <property name="pc" value="0" />
    </component>
    <component name="memoryMap" class="MemoryMap16bit">
        <connection name="" ref="floatingBus" />
        <connection name="" ref="memory0000" />
        <connection name="" ref="memory8000" />
        <connection name="" ref="io" />
        <connection name="" ref="slotMemory" />
        <connection name="" ref="slotExpansionMemory" />
    </component>
    <component name="mmu" class="AppleIIMMU">
        <connection name="hostSystem" ref="host::system" />
        <connection name="memoryMap" ref="memoryMap" />
        <connection name="floatingBus" ref="floatingBus" />
        <connection name="romD0" ref="romD0" />
        <connection name="romD8" ref="romD8" />
        <connection name="romE0" ref="romE0" />
        <connection name="romE8" ref="romE8" />
        <connection name="romF0" ref="romF0" />
        <connection name="romF8" ref="romF8" />
        <connection name="romSocket" ref="" />
        <property name="inh" value="0" />
    </component>
    <component name="floatingBus" class="AppleIIFloatingBus">
        <connection name="system" ref="system" />
    </component>
    <component name="memory0000" class="RAM">
        <data name="image" src="${DEVICE_NAME}.bin" />
        <property name="map" value="0x0000-0x7fff" />
        <property name="size" value="0x4000" />
    </component>
    <component name="memory8000" class="RAM">
        <data name="image" src="${DEVICE_NAME}.bin" />
        <property name="map" value="0x0000-0x3fff" />
        <property name="size" value="0x4000" />
    </component>
    <component name="io" class="MemoryMap8bit">
        <property name="map" value="0xc000" />
        <connection name="" ref="floatingBus" />
        <connection name="" ref="keyboard" />
        <connection name="" ref="speaker" />
        <connection name="" ref="video" />
        <connection name="" ref="game" />
        <connection name="" ref="cassetteIn" />
    </component>
    <component name="keyboard" class="AppleIIKeyboard">
        <property name="map" value="0x00-0x1f" />
        <connection name="hostKeyboard" ref="host::keyboard" />
        <connection name="floatingBus" ref="floatingBus" />
    </component>
    <component name="speaker" class="DAC1bit">
        <property name="value" value="0" />
        <property name="map" value="0x20-0x2f" />
        <property name="map" value="0x30-0x3f" />
        <connection name="hostAudio" ref="host::audio" />
        <connection name="floatingBus" ref="floatingBus" />
    </component>
    <component name="videoMemory" class="AppleIIVideoMemory">
    </component>
    <component name="video" class="AppleIIVideo">
        <property name="map" value="0x50-0x57" />
        <resource name="image" src="roms/Signetics 2513 Video ROM.bin" />
        <connection name="mmu" ref="mmu" />
        <connection name="monitor" ref="" />
        <connection name="romSocket" ref="" />
    </component>
    <component name="game" class="AppleIIGamePort">
        <property name="map" value="0x58-0x7f" />
        <property name="an0" value="0" />
        <property name="an1" value="0" />
        <property name="an2" value="0" />
        <property name="an3" value="0" />
        <connection name="cassetteIn" ref="cassetteIn" />
        <connection name="port" ref="" />
    </component>
    <component name="cassetteIn" class="AppleIICassetteIn">
        <property name="map" value="0x60-0x60" />
        <connection name="hostAudio" ref="host::audio" />
    </component>
    <component name="slotMemory" class="AppleIISlotMemory">
        <property name="map" value="0xc100-0xc7ff" />
        <connection name="slotExpansionMemory" ref="slotExpansionMemory" />
        <connection name="io" ref="io" />
        <connection name="system" ref="system" />
        <connection name="floatingBus" ref="floatingBus" />
        <connection name="mmu" ref="mmu" />
        <connection name="memoryMap" ref="memoryMap" />
        <connection name="slot0" ref="" />
        <connection name="slot1" ref="" />
        <connection name="slot2" ref="" />
        <connection name="slot3" ref="" />
        <connection name="slot4" ref="" />
        <connection name="slot5" ref="" />
        <connection name="slot6" ref="" />
        <connection name="slot7" ref="" />
    </component>
    <component name="slotExpansionMemory" class="AppleIISlotExpansionMemory">
        <property name="map" value="0xc800-0xcfff" />
        <connection name="floatingBus" ref="floatingBus" />
    </component>
    <component name="romD0" class="ROM">
        <property name="map" value="0xd000-0xd7ff" />
        <resource name="image" src="roms/Apple II Programmers Aid ROM.bin" />
    </component>
    <component name="romD8" class="ROM">
        <property name="map" value="0xd800-0xdfff" />
        <resource name="image" src="roms/Apple II Programmers Aid ROM.bin" />
    </component>
    <component name="romE0" class="ROM">
        <property name="map" value="0xe000-0xe7ff" />
        <resource name="image" src="roms/Apple II E0 ROM.bin" />
    </component>
    <component name="romE8" class="ROM">
        <property name="map" value="0xe800-0xefff" />
        <resource name="image" src="roms/Apple II E8 ROM.bin" />
    </component>
    <component name="romF0" class="ROM">
        <property name="map" value="0xf000-0xffff" />
        <resource name="image" src="roms/Apple II F0 ROM.bin" />
    </component>
    <component name="romF8" class="ROM">
        <property name="map" value="0xf800-0xffff" />
        <resource name="image" src="roms/Apple II F8 ROM.bin" />
    </component>
</device>

</dml>
