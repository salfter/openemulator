<device id="appleMac128" label="Macintosh 128K" image="images/Apple/Apple Macintosh.png" group="system">
</device>
<port id="appleMac128.disk" ref="" type="Apple Disk Drive" label="Disk Drive Port" image="images/Connectors/DB19 Female.png">
</port>
<port id="appleMac128.serial" ref="" type="RS-422" label="Printer Port" image="images/Connectors/DE9 Female.png">
</port>
